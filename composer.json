{
  "name": "rickselby/laravel-external-basic-auth",
  "description": "External Basic Auth for Laravel",
  "keywords": ["laravel", "auth", "basic", "external"],
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Rick Selby",
      "email": "rick@selby-family.co.uk"
    }
  ],
  "require": {
    "php": "8.*",
    "laravel/framework": "10.*|11.*"
  },
  "require-dev": {
    "graham-campbell/testbench": "6.*",
    "phpunit/phpunit": "11.*"
  },
  "autoload": {
    "psr-4": {
      "RickSelby\\Laravel\\Auth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RickSelby\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RickSelby\\Laravel\\Auth\\ExternalBasicAuthServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": false
  }
}
